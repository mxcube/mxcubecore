<object class="TINEMotor">
  <command type="tine" name="setPosition" tinename="/PE2/collection/distance">move</command>
  <command type="tine" name="stopAxis" tinename="/PE2/collection/distance">stop</command>
  <channel type="tine" name="axisState" tinename="/PE2/collection/distance">state2</channel>
  <channel type="tine" name="axisPosition" tinename="/PE2/collection/distance">position</channel>
  <channel type="tine" name="axisLimits" tinename="/PE2/collection/distance">limits</channel>

  <epsilon>0.01</epsilon>
  <verboseUpdate>False</verboseUpdate>
  <maxMotorPosition>1000</maxMotorPosition>
</object>
